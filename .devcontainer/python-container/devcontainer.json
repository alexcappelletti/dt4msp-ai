{
  "name": "Python App Container",
  "dockerComposeFile": ["../../docker-compose.yaml"],
  "service": "app",
  "workspaceFolder": "/app",
  "shutdownAction": "none",
  "customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	},

	// Uncomment to connect as root instead. More info: https://aka.ms/dev-containers-non-root.
	"remoteUser": "root"

}